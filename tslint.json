{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "only-arrow-functions": [true],
        "promise-function-async": true,
        "no-console": [false],
        "no-bitwise": false,
        "prefer-const": true,
        "no-unused-variable": true,
        "max-classes-per-file": [
            false
        ],
        "no-floating-promises": true
    },
    "rulesDirectory": []
}