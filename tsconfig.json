{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": [
            "es5",
            "dom",
            "es2015.promise"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "noEmitOnError": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "declaration": true
    }
}
